DROP TABLE IF EXISTS "GROUP";
CREATE TABLE "GROUP" ("groupID" CHAR PRIMARY KEY  NOT NULL ,"name" text DEFAULT (null) ,"lastupdate" DATETIME);
DROP TABLE IF EXISTS "GROUP_MEMBER";
CREATE TABLE "GROUP_MEMBER" ("groupID" CHAR PRIMARY KEY  NOT NULL ,"userID" CHAR NOT NULL ,"nickname" text DEFAULT (null) );
DROP TABLE IF EXISTS "MESSAGE";
CREATE TABLE "MESSAGE" ("messageID" CHAR PRIMARY KEY  NOT NULL  DEFAULT (null) ,"userID" CHAR NOT NULL  DEFAULT (null) ,"time" DOUBLE DEFAULT (null) ,"attachment" text DEFAULT (null) ,"content" text DEFAULT (null) ,"groupID" CHAR);
DROP TABLE IF EXISTS "USER";
CREATE TABLE "USER" ("userID" CHAR PRIMARY KEY  NOT NULL ,"username" text DEFAULT (null) ,"avatarurl" VARCHAR, "email" TEXT, "lastLogonTime" DOUBLE);
